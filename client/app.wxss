.phone{
    display: flex;
    justify-content: center;
    align-items: center;
    width: 100%;
    height: 600rpx;
}

.phone-container{
    position: relative;
    width: 144rpx;
    height: 256rpx;
}

.phone-container .phone-bg{
    position: absolute;
    width: 100%;
    height: 100%;
    background: url(data:image/png;base64,);
    background-size: 100% 100%;
}
.phone-container .phone-content{
    margin-top: 10px;
    width: 144rpx;
    height: 200rpx;
    background: rgba(0,0,0,0.7);
    z-index: 1;
}

.canvas-element{
    position: absolute;
    width: 100%;
    height: 120px;
    z-index: 1;
}

.form-section{
    width: 300px;
    box-sizing: border-box;
    padding-top: 120rpx;
    margin-left: auto;
    margin-right: auto;
}

.form-section input{
    margin-bottom: 78rpx;
    height: 65rpx;
    border-bottom: 1px solid #adadae;
}


.btn-container{
    position:relative;
    display:block;
    margin-left:auto;
    margin-right:auto;
    height: 92rpx;
    box-sizing:border-box;
    font-size:18px;
    text-align:center;
    text-decoration:none;
    line-height:2.55555556;
    border-radius: 18px;
    -webkit-tap-highlight-color:transparent;
    overflow:hidden;
    color:#000000;
    background-image: linear-gradient(135deg, #8b559f 50px,#0b4e8d);
}
.btn-container .btn{
    width: 296px;
    height: 84rpx;
    background:#fff;
    margin-top:2px;
    margin-left: 2px;
    border-radius: 17px;
    color: #303030;
}